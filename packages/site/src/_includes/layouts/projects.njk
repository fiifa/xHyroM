---
layout: layouts/base.njk
---

<h1>My Projects</h1>


{% for project in projects %}
    <h3>{{ project.name }}</h3>
    <hr />
    
    {{ project.description | md | safe }}

    <p>Stack:</p>
    <ul>
        {% for stack in project.stack %}
            <li>{{ stack | safe }}</li>
        {% endfor %}
    </ul>

    <p>Links:</p>
    <ul>
        {% for link in project.links %}
            <li>
                <a href='{{ link.url }}' target='_blank'>{{ link.type }}</a>
            </li>
        {% endfor %}
    </ul>
{% endfor %}
